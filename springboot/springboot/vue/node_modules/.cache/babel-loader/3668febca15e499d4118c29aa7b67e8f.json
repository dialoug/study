A;AAEA,UAAA,QAAA,EAAA,KAAA,QAFA;AAGA;AACA,UAAA,SAAA,EAAA,KAAA,IAAA,CAAA;AAJA;AADA,OAAA,EAOA,IAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAFA,CAEA;AACA,OAVA;AAWA,KAbA;AAcA,IAAA,KAdA,mBAcA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KAjBA;AAkBA;AACA,IAAA,gBAnBA,4BAmBA,QAnBA,EAmBA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KAtBA;AAuBA;AACA,IAAA,mBAxBA,+BAwBA,OAxBA,EAwBA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KA3BA;AA4BA,IAAA,IA5BA,gBA4BA,GA5BA,EA4BA;AAAA;;AACA,WAAA,IAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,WAAA,IAAA,CAAA,UAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,eAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA;;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA;AACA,OALA;AAMA;AArCA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n<!--    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>-->\r\n    <el-table :data=\"tableData\" max-height=\"680\">\r\n      <el-table-column type=\"expand\">\r\n        <template v-slot=\"scope\">\r\n          <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n            <el-form-item label=\"患者id\"><span>{{ scope.row.patientId }}</span></el-form-item>\r\n            <el-form-item label=\"医生id\"><span>{{ scope.row.doctorId }}</span></el-form-item>\r\n            <el-form-item label=\"挂号时间\"><span>{{ scope.row.createTime }}</span></el-form-item>\r\n            <el-form-item label=\"结束时间\"><span>{{ scope.row.endTime }}</span></el-form-item>\r\n            <el-form-item label=\"病因\"><span>{{ scope.row.etiology }}</span></el-form-item>\r\n            <el-form-item label=\"药物\"><span>{{ scope.row.drug }}</span></el-form-item>\r\n            <el-form-item label=\"检测项目\"><span>{{ scope.row.checks }}</span></el-form-item>\r\n            <el-form-item label=\"建议\"><span>{{ scope.row.advice }}</span></el-form-item>\r\n            <el-form-item label=\"总价\"><span>{{ scope.row.totalPrice }}</span></el-form-item>\r\n          </el-form>\r\n\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"id\" label=\"挂号单\" width=\"80\" sortable></el-table-column>\r\n      <el-table-column prop=\"state\" label=\"挂号状态\">\r\n        <template v-slot=\"scope\">\r\n          <el-tag v-if=\"scope.row.state === '1'\" type=\"success\">已完成</el-tag>\r\n          <el-tag v-else-if=\"scope.row.state ==='0'\" type=\"danger\">未完成</el-tag>\r\n          <el-tag v-else type=\"warning\">已过期</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"priceState\" label=\"缴费状态\">\r\n        <template v-slot=\"scope\">\r\n          <el-tag v-if=\"scope.row.priceState === '1'\" type=\"success\">已完成</el-tag>\r\n          <!-- 如果挂号已完成且未缴费则提供缴费按钮 -->\r\n          <el-button v-else-if=\"scope.row.state === '1' & scope.row.priceState === '0'\" type=\"warning\" @click=\"save(scope.row)\">点击缴费</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"MyRegister\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      // name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.request.get(\"/registration/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          // name: this.name,\r\n          patientId: this.user.id\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total  // 记录总数\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    // 修改每页记录条数\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    // 选择页码\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    save(row){\r\n      this.form.id = row.id\r\n      this.form.priceState = '1'\r\n      this.request.post(\"/registration\",this.form).then(res => {\r\n        if (res.code === '200'){\r\n          this.load()\r\n          this.$message.success('缴费成功')\r\n        }\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee !important;\r\n}\r\n\r\n.demo-table-expand {\r\n  font-size: 0;\r\n}\r\n\r\n.demo-table-expand label {\r\n  width: 90px;\r\n  color: #99a9bf;\r\n}\r\n\r\n.demo-table-expand .el-form-item {\r\n  margin-right: 0;\r\n  margin-bottom: 0;\r\n  width: 50%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/patient"}]}